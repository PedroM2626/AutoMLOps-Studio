# Configurações Gerais
API_SECRET_KEY=your_secret_key_here
LOG_LEVEL=INFO

# Configurações MLflow (Local ou Remoto)
# Para local: sqlite:///mlflow.db
# Para DagsHub: https://dagshub.com/<username>/<repo>.mlflow
MLFLOW_TRACKING_URI=sqlite:///mlflow.db

# Credenciais DagsHub (Opcional, se usar MLflow remoto)
MLFLOW_TRACKING_USERNAME=your_dagshub_username
MLFLOW_TRACKING_PASSWORD=your_dagshub_token_or_password

# Diretórios
MODEL_REGISTRY_PATH=./models
DATA_LAKE_PATH=./data_lake
